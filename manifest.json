{
    "manifest_version": 2,
    "name": "Note Here!",
    "version": "1.0",
    "description": "Notepad for notes",
    "permissions": ["storage"],
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "browser_action": {
        "default": "icon16.png",
        "default_popup": "popup.html"
    }
}